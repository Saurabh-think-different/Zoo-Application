
<h1 style="color:green; text-align: center;">Successfully Added Animal</h1>


<div class="showcase">
    
    <table>
        <tr>
            <td><h3>Animal ID: </h3></td>
            <td><span><h5><%= aid %></span></h5></td>
        </tr>
        <tr>
            <td><h3>Animal Name: </h3></td>
            <td><span><h5><%= data.aname %></span></h5></td>
        </tr>
        <tr>
            <td><h3>Common Name: </h3></td>
            <td><h5><%= data.cname %> </h5></td>
        </tr>
        <tr>
            <td><h3>Biological Name: </h3></td>
            <td><h5><%= data.bioname %> </h5></td>
        </tr>
        <tr>
            <td><h3>Gender: </h3></td>
            <td><h5><%= data.gender %></h5> </td>
        </tr>
        <tr>
            <td><h3>Height: </h3></td>
            <td><h5><%= data.ht %> </h5></td>
        </tr>
        <tr>
            <td><h3>Weight: </h3></td>
            <td><h5><%= data.wt %></h5> </td>
        </tr>
        <tr>
            <td><h3>Feed ID: </h3></td>
            <td><h5><%= data.feedid %></h5> </td>
        </tr>
        <tr>
            <td><h3>Date Of Birth: </h3></td>
            <td><h5><%= data.dob %> </h5></td>
        </tr>
        <tr>
            <td><h3>Cage No: </h3></td>
            <td><h5><%= data.cageno %> </h5></td>
        </tr>
        

    </table>
    
</div>

<div class="btn">
    <a href="./animals_add">EDIT</a>
</div>
