<br>
<h2 class="container">Search Employee</h2>

  <div class="container">
    <br>
    <form action="/emp_search" method="get">
      <label class="form-label">Enter EmpID :</label> 
        <input type="text" name="empid" class="form-control" value="">
        <br>
        <button type="submit" class="btn btn-dark">Search</button>
    </form>
    <br>
    <div class="table-responsive">
      <table class="empsearchtable table table-bordered ">
        <thead class="table-dark">
          <th>EmpID</th>
          <th>FName</th>
          <th>LName</th>
          <th>Gender</th>
          <th>House No</th>
          <th>Street Name</th>
          <th>Area</th>
          <th>City</th>
          <th>Zip</th>
          <th>Phone</th>
          <th>DOB</th>
          <th>Dept</th>
          <th>Salary</th>
        </thead>
        <tbody>
        </tbody>
      </table>
      <br>
    </div>
    
  </div>

<script src="/public/js/search.js"></script>